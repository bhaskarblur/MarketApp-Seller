    android:layout_width="match_parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView26"
    app:layout_constraintBottom_toBottomOf="parent"
        android:orientation="vertical">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent">

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:text="GST number"
        android:visibility="gone"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:id="@+id/textView59"
        app:layout_constraintTop_toBottomOf="@+id/panet2"
        />

    <EditText
        android:background="@drawable/descboxbg"
        android:layout_height="45dp"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="5dp"
        android:layout_marginStart="20dp"
        android:id="@+id/gstet"
        app:layout_constraintTop_toBottomOf="@+id/textView59"
        android:textColorHint="#717171"
        android:paddingLeft="15dp"
        android:inputType="textCapCharacters"
        android:hint="Enter"
        android:visibility="gone"
        android:maxLength="15"
        android:ems="10"
        android:backgroundTint="@color/white"
        android:layout_marginEnd="20dp"
        />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:text="PAN number"
        android:visibility="gone"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:id="@+id/textView60"
        app:layout_constraintTop_toBottomOf="@+id/moaet"
        />

    <EditText
        android:background="@drawable/descboxbg"
        android:layout_height="45dp"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="5dp"
        android:layout_marginStart="20dp"
        android:id="@+id/panet2"
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@+id/textView60"
        android:textColorHint="#717171"
        android:paddingLeft="15dp"
        android:inputType="textCapCharacters"
        android:hint="Enter"
        android:maxLength="10"
        android:ems="10"
        android:backgroundTint="@color/white"
        android:layout_marginEnd="20dp"/>

    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent" android:textSize="16sp" android:textColor="@color/black" android:text="Delivery Radius (Km)" android:fontFamily="@font/poppins" android:layout_marginTop="30dp" android:layout_marginStart="20dp" android:id="@+id/textView7"/>

    <EditText android:background="@drawable/descboxbg" android:layout_height="45dp" android:layout_width="match_parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent" android:textSize="15sp" android:textColor="@color/black" android:fontFamily="@font/poppins" android:layout_marginTop="5dp" android:layout_marginStart="20dp" android:id="@+id/shopradet" app:layout_constraintTop_toBottomOf="@+id/textView7" android:textColorHint="#717171" android:paddingLeft="15dp" android:inputType="number" android:hint="Enter" android:ems="10" android:backgroundTint="@color/white" android:layout_marginEnd="20dp"/>

    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" app:layout_constraintStart_toStartOf="parent" android:textSize="16sp" android:textColor="@color/black" android:text="Minimum Order Amount (Rs)" android:fontFamily="@font/poppins" android:layout_marginTop="20dp" android:layout_marginStart="20dp" android:id="@+id/minordet" app:layout_constraintTop_toBottomOf="@+id/shopradet"/>

    <EditText android:background="@drawable/descboxbg" android:layout_height="45dp" android:layout_width="match_parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent" android:textSize="15sp" android:textColor="@color/black" android:fontFamily="@font/poppins" android:layout_marginTop="5dp" android:layout_marginStart="20dp" android:id="@+id/moaet" app:layout_constraintTop_toBottomOf="@+id/minordet" android:textColorHint="#717171" android:paddingLeft="15dp" android:inputType="number" android:hint="Enter" android:ems="10" android:backgroundTint="@color/white" android:layout_marginEnd="20dp"/>

    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" app:layout_constraintStart_toStartOf="parent" android:textSize="16sp" android:textColor="@color/black" android:text="Free Delivery Above (Rs)" android:fontFamily="@font/poppins" android:layout_marginTop="20dp" android:layout_marginStart="20dp" android:id="@+id/textView10" app:layout_constraintTop_toBottomOf="@+id/gstet"/>

    <EditText android:background="@drawable/descboxbg" android:layout_height="45dp" android:layout_width="match_parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent" android:textSize="15sp" android:textColor="@color/black" android:fontFamily="@font/poppins" android:layout_marginTop="5dp" android:layout_marginStart="20dp" android:id="@+id/freedelet" app:layout_constraintTop_toBottomOf="@+id/textView10" android:textColorHint="#717171" android:paddingLeft="15dp" android:inputType="none|number" android:hint="Enter" android:ems="10" android:backgroundTint="@color/white" android:layout_marginEnd="20dp"/>

    <EditText android:background="@drawable/descboxbg" android:layout_height="45dp" android:layout_width="match_parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent" android:textSize="15sp" android:textColor="@color/black" android:fontFamily="@font/poppins" android:layout_marginTop="5dp" android:layout_marginStart="20dp" android:id="@+id/delvcharet" app:layout_constraintTop_toBottomOf="@+id/textView49" android:textColorHint="#717171" android:paddingLeft="15dp" android:inputType="none|number" android:hint="Enter" android:ems="10" android:backgroundTint="@color/white" android:layout_marginEnd="20dp" android:visibility="visible"/>

    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" app:layout_constraintStart_toStartOf="parent" android:textSize="16sp" android:textColor="@color/black" android:text="Delivery Charges (Above minimum order)" android:fontFamily="@font/poppins" android:layout_marginTop="20dp" android:layout_marginStart="20dp" android:id="@+id/textView49" app:layout_constraintTop_toBottomOf="@+id/freedelet"/>

    <EditText android:background="@drawable/descboxbg" android:layout_height="45dp" android:layout_width="match_parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent" android:textSize="16sp" android:textColor="@color/black" android:fontFamily="@font/poppins" android:layout_marginTop="5dp" android:layout_marginStart="20dp"
        android:id="@+id/upiidet" app:layout_constraintTop_toBottomOf="@+id/textView50" android:textColorHint="#717171" android:paddingLeft="15dp" android:inputType="textPersonName" android:hint="Enter" android:ems="10" android:backgroundTint="@color/white" android:layout_marginEnd="20dp"/>

     <TextView
         android:layout_width="300dp"
         android:layout_height="wrap_content"
         android:layout_marginStart="20dp"
         android:layout_marginTop="15dp"
         android:fontFamily="@font/poppins"
         android:textSize="12sp"
         android:textColor="@color/primcolor"
         android:textStyle="bold"
         android:inputType="textMultiLine"
         android:text="Note: Enter all the details."
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toBottomOf="@id/upiidet"
         />

    <TextView android:layout_height="wrap_content" android:layout_width="142dp"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="16sp" android:textColor="@color/black"
        android:text="UPI" android:fontFamily="@font/poppins" android:layout_marginTop="20dp" android:layout_marginStart="20dp" android:id="@+id/textView50" app:layout_constraintTop_toBottomOf="@+id/delvcharet"/>

    <Button
        android:background="@drawable/loginbtnbg"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:text="Update"
        android:layout_marginTop="60dp"
        android:layout_marginStart="50dp"
        android:id="@+id/shsetupd"
        app:layout_constraintTop_toBottomOf="@+id/upiidet" app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="50dp" app:layout_constraintVertical_bias="1.0" android:textAllCaps="false" android:foreground="?android:selectableItemBackground" android:layout_marginBottom="50dp"/>

    <ProgressBar android:layout_height="wrap_content" android:layout_width="wrap_content" app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="50dp" android:id="@+id/progressBar3" app:layout_constraintTop_toBottomOf="@+id/upiidet" android:visibility="gone" app:layout_constraintHorizontal_bias="0.5" android:progressTint="@color/primcolor" android:progressDrawable="@color/primcolor" android:indeterminateTint="@color/primcolor" style="?android:attr/progressBarStyle"/>

    <ImageView android:layout_height="30dp" android:layout_width="50dp" app:layout_constraintEnd_toEndOf="parent" android:textColor="@color/black" android:text="Add Image" android:id="@+id/gstimgbtn" android:layout_marginEnd="30dp" android:visibility="invisible" android:textAllCaps="false" android:layout_marginBottom="15dp" app:srcCompat="@drawable/imgicon" app:layout_constraintBottom_toTopOf="@+id/gstet" android:clickable="true"/>

    <ImageView android:layout_height="30dp" android:layout_width="50dp" app:layout_constraintEnd_toEndOf="parent" android:textColor="@color/black" android:text="Add Image" android:id="@+id/panimgbtn" android:layout_marginEnd="30dp" android:visibility="invisible" android:textAllCaps="false" android:layout_marginBottom="15dp" app:srcCompat="@drawable/imgicon" app:layout_constraintBottom_toTopOf="@+id/panet2" android:clickable="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>

